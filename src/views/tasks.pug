extends layout


block main_content
    .contenido-principal
        hgroup 
            h1=project.name
            h3 (#{ name_page })
        pre #{ vardump( project ) }
        .contenedor-acciones
            a(href=`/project/edit/${ project.id }` class="boton") Editar proyecto
            button#eliminar-proyecto(data-project-url=project.url type="button" class="boton eliminar") Eliminar proyecto